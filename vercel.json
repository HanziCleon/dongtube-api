{
  "rewrites": [
    {
      "source": "/",
      "destination": "/home"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/streaming-:slug",
      "destination": "/watch.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/home",
      "permanent": true
    },
    {
      "source": "/admin.html",
      "destination": "/admin",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
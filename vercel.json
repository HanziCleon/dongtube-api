{
  "version": 2,
  "name": "dongtube",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/dongtube"
    },
    {
      "src": "^/dongtube/?$",
      "dest": "/index.html"
    },
    {
      "src": "^/watch/(.+)=id:([0-9]+)(&ep=([0-9]+))?$",
      "dest": "/watch.html?id=$2&ep=$4"
    },
    {
      "src": "^/watch/?$",
      "dest": "/watch.html"
    },
    {
      "src": "^/admin/?$",
      "dest": "/index.html"
    },
    {
      "src": "^/(.*)\\.html$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/dongtube",
      "permanent": true
    },
    {
      "source": "/watch.html",
      "destination": "/watch",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}